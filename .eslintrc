{
  "extends": "airbnb-base",
  "env": {
      "node": true
  },
  "rules": {
      "no-console": 0,
      "linebreak-style": 0,
      "global-require": 0,
      "eslint linebreak-style": [0, "error", "window "],
      "import/extensions": [
          "error",
          "ignorePackages",
          {
            "js": "always"
          }
       ]
  }
}